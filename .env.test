# Test environment configuration
NODE_ENV=test
LOG_LEVEL=error

# Test API keys (mock values)
EASYPOST_API_KEY=mock
VEEQO_API_KEY=mock

# Disable external integrations in tests
ENABLE_PERFORMANCE_MONITORING=false
ENABLE_API_LOGGING=false
MOCK_API_CALLS=true

# Fast test timeouts
API_TIMEOUT=5000
MAX_API_RETRIES=1

# Test cache settings
RATE_CACHE_TTL=5
INVENTORY_CACHE_TTL=5

# Test thresholds (lower for testing)
MEMORY_WARNING_THRESHOLD=100
MEMORY_ERROR_THRESHOLD=200
API_WARNING_THRESHOLD=1000
API_ERROR_THRESHOLD=2000