{
  "source": {
    "include": ["./src/", "./README.md"],
    "includePattern": "\\.(js|ts)$",
    "exclude": ["node_modules/", "dist/", "test/"]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown",
    "@typescript-eslint/parser"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true
  },
  "typescript": {
    "moduleRoot": "./src"
  },
  "metadata": {
    "title": "Unified EasyPost-Veeqo MCP Server API Documentation",
    "description": "API documentation for the FastMCP server integrating EasyPost shipping and Veeqo inventory management with AI-powered optimization."
  },
  "fastmcp": {
    "generateToolDocs": true,
    "generateResourceDocs": true,
    "generatePromptDocs": true,
    "includeExamples": true,
    "includeSchemas": true
  }
}